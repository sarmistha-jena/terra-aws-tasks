{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowWriteAccessToSpecificBucket",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::${bucket_name}/*"
        }
    ]
}